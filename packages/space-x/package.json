{
  "name": "space-x",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "rimraf dist && next build --profile",
    "format": "prettier --write pages/**/*.{ts,tsx,scss} components/**/*.{ts,tsx,scss} common/**/*.{ts,tsx} helpers/**/*.{ts,tsx}",
    "lint": "eslint --ext js,jsx,ts,tsx --color --fix .",
    "precommit": "pretty-quick --staged",
    "win-find-port": "netstat -ano | findstr :3000",
    "win-kill-port": "tskill <id>",
    "localbuild": "yarn format & yarn lint & rimraf dist && yarn build"
  },
  "dependencies": {
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "next": "^10.2.0",
    "next-pwa": "^5.2.21",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.32.12"
  },
  "devDependencies": {
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "typescript": "^4.2.4"
  }
}
